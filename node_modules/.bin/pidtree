#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cubeddu/neptune/apps/graph-explorer/node_modules/pidtree/bin/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/pidtree/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules:/Users/cubeddu/neptune/apps/node_modules:/Users/cubeddu/neptune/node_modules:/Users/cubeddu/node_modules:/Users/node_modules:/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cubeddu/neptune/apps/graph-explorer/node_modules/pidtree/bin/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/pidtree/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules:/Users/cubeddu/neptune/apps/node_modules:/Users/cubeddu/neptune/node_modules:/Users/cubeddu/node_modules:/Users/node_modules:/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pidtree/bin/pidtree.js" "$@"
else
  exec node  "$basedir/../pidtree/bin/pidtree.js" "$@"
fi
