#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.19.6_babel-jest@28.1.3_jest@28.1.3_typescript@4.8.4/node_modules/ts-jest/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.19.6_babel-jest@28.1.3_jest@28.1.3_typescript@4.8.4/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.19.6_babel-jest@28.1.3_jest@28.1.3_typescript@4.8.4/node_modules/ts-jest/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.19.6_babel-jest@28.1.3_jest@28.1.3_typescript@4.8.4/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
