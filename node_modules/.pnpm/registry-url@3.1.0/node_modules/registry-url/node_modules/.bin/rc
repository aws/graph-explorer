#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/rc@1.2.8/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/rc@1.2.8/node_modules:/Users/cubeddu/neptune/apps/graph-explorer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
else
  exec node  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
fi
